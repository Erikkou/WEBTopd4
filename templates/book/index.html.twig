{% extends 'base.html.twig' %}
{% block title %}Books{% endblock %}

{% block body %}
    <h1>List of Books</h1>

    <ul>
        {% for book in books %}
            <li>{{ book.title }} by {{ book.author }}</li>
        {% else %}
            <li>No books found.</li>
        {% endfor %}
    </ul>
{% endblock %}
